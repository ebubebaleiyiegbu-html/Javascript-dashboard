// passing values to function with augments
function myFunctionWithArgs(a, b){
    console.log(a - b);
}
myFunctionWithArgs(10, 5) //outputs = 5

function myFunctionWithArgs(a, b) {
    console.log(a + b);
}
myFunctionWithArgs(10, 5); //outputs + 15

// global scopes and functions***
var myGlobal = 10;


// global vs local scope funsctio
var outerWear = "T-shirt"; // global vairable

function myOutfit() {
    var outerWear = "sweater"; // locial variable
    return outerWear
}
console.log(myOutfit()); // outputs sweater
console.log(outerWear); // outputs T-shirt

// return a value from a function with return
function minusTen(num) {
    return num -10
}
console.log(minusTen(20)); //outputs 10

function minusFifty(num) {
    return num -30
}
console.log (minustFifty(10)); //outputs -20

// assignment with a returned value
var changed = 0;
function change(num) {
    return (num + 5) /3;// changed is 5
}
changed = change(10); // changed now equals 5

// stand in line***
 function nextInLine(arr, item) { // add the item to the end of the array
    arr.push(item); // adds the item to the end of the array
    return arr.shift(); // returns the first item in the array
 }
var testArr = [1, 2, 3, 4, 5]; // testArr is [1, 2, 3, 4, 5]

console.log("Before: " + JSON.stringify(testArr)); // outputs [1, 2, 3, 4, 5]
console.log(nextInLine(testArr, 6)); // outputs 1
console.log("After: " + JSON.stringify(testArr)); // outputs [2, 3, 4, 5, 6]

